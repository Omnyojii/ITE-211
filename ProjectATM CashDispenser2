public class CashDispenser {
	private final static int INITIAL_COUNT = 10000000;
	private int count;

    public CashDispenser() {
    	count = INITIAL_COUNT;
    }
    
    public void dispenseCash(int amount){
    	count -= amount;
    }
    
    public boolean CashAvailable(int amount){  	
    	if( count >= amount){
    		return true;
    	}
    	else{
    		return false;
    	}
    }
}
