    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
            FirstName.setText(null);
            LastName.setText(null);
            Title.setText(null);
            NickName.setText(null);
            Address.setText(null);
            Email.setText(null);
            ContactNumber.setText(null);
            CheckIn.setModel(new javax.swing.SpinnerDateModel());
            CheckOut.setModel(new javax.swing.SpinnerDateModel());
            BedType.setModel(new javax.swing.DefaultComboBoxModel<>(new String[] {"","Single", "Double", "Queen"}));
            Floors.setModel(new javax.swing.SpinnerNumberModel(1, 1, 4, 1));
            Hours.setModel(new javax.swing.SpinnerNumberModel(12, 12, null, 12));
            Rooms.setModel(new javax.swing.SpinnerNumberModel(1, 1, null, 1));
            Discounts.setModel(new javax.swing.DefaultComboBoxModel<>(new String[] { "none", "Seniors", "Event", "Loyalty" }));
            Deposit.setText("0.0");
            ExtraCharges.setText("0.0");
            Total.setText("0.0");
            
    }                                        


    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        // Guest INFO
        DefaultTableModel model = (DefaultTableModel)myTable.getModel();
        
        if("".equals(FirstName.getText()) || "".equals(LastName.getText()) || "".equals(Title.getText()) || "".equals(NickName.getText()) || "".equals(Address.getText()) || "".equals(Email.getText()) || "".equals(ContactNumber.getText()) || "".equals(BedType.getSelectedItem())){
            JOptionPane.showMessageDialog(null,"Please complete the form.");
        }
        else{
            String randomNum = UUID.randomUUID().toString().replaceAll("-", "");
            BookingID.setText(randomNum);
            
            
            
            
            model.addRow(new Object[]{
                    FirstName.getText(),
                    LastName.getText(),
                    Title.getText(),
                    NickName.getText(),
                    Address.getText(),
                    Email.getText(),
                    ContactNumber.getText(),
                    BookingID.getText(),
                    CheckIn.getValue(),
                    CheckOut.getValue(),
                    BedType.getSelectedItem(),
                    Floors.getValue(),
                    Hours.getValue(),
                    Rooms.getValue(),
                    Discounts.getSelectedItem(),
                    Deposit.getText(),
                    ExtraCharges.getText(),
                    Total.getText(),
                    "Delete"
                }
                    
            
            );
            
            JOptionPane.showMessageDialog(null," Registration Succesful! ");
        }       
        
        
       
    }                                        

