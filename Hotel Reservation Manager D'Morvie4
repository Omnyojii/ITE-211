//////////////////////////////////////////////////////////////////////////////////////
    private void calculation(){   
        //DISCOUNTS
        double discounted = 0;
        int bedType = 0;
        if(Discounts.getSelectedItem() == "Seniors"){
            discounted = 0.20;
        }
        if(Discounts.getSelectedItem() == "Event"){
            discounted = 0.25;
        } 
        if(Discounts.getSelectedItem() == "Loyalty"){
            discounted = 0.30;
        }
        //BEDTYPE
        if(BedType.getSelectedItem()== "Single"){
            Floors.setModel(new javax.swing.SpinnerNumberModel(3, 3, 4, 1));
            bedType = 200;
        }
        else if(BedType.getSelectedItem()== "Double"){
            Floors.setModel(new javax.swing.SpinnerNumberModel(1, 1, 4, 1));
            bedType = 250;
        }
        else if(BedType.getSelectedItem()== "Queen"){  
            Floors.setModel(new javax.swing.SpinnerNumberModel(1, 1, 2, 1));
            bedType = 300;
        }
        else{
            Floors.setModel(new javax.swing.SpinnerNumberModel(1, 1, 2, 1));
        }
                
                
        //BEDTYPE//HOURS//ROOMS//TOTAL
        int convert = (Integer)Hours.getValue() / 12;
        int hours = bedType * convert;
        int rooms = hours * (Integer)Rooms.getValue();
        double discAmount = rooms - (rooms*discounted);
            
        //DEPOSIT
        String newDeposit1 = Deposit.getText();            
        double newDeposit2 = Double.parseDouble(newDeposit1);
            
        //EXTRA CHARGES
        String newCharges1 = ExtraCharges.getText();            
        double newCharges2 = Double.parseDouble(newCharges1);
            
        //TOTAL
        double newTotal = (newCharges2+discAmount)-newDeposit2;
        Total.setText(Double.toString(newTotal));
    }
        
